interface Shape 
{
  abstract double getArea();
}
class Rectangle implements Shape
{
  double length,width;
  
  public getArea(double length,double width)
  {
    this.length=length;
    this.width=width;
  }
  
  public string toString()
  {
    String answer=double.toString(length*width);
    return answer;
  }
}

class Tritangle implements Shape
{
  double base,height;
  
  public getArea(double base,double height)
  {
    this.base=base;
    this.height=height;
  }
  
  public string toString()
  {
    String answer=double.toString(base*height);
    return answer;
  }
}
